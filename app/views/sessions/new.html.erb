<h1 id="session-h1">登录</h1>
<%= form_for(:session,url: sessions_path) do |f| %>
<div class="row">
	<div class="large-6 columns large-centered">
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>
</div>

<div class="row">
	<div class="large-6 columns large-centered">
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>
</div>
<div class="row">
	<div class="large-6 columns large-centered">
		<%= f.submit "登录", class: "button success" %>
<p>还没有注册？<%= link_to '注册',new_user_path %></p>  
	</div>
</div>
<% end %>
